<script type="text/javascript">
    $(function () {
        var $ul = $("#j-scroll ul"),
            $lf = $("#j-scroll span.btn-left"),
            $rt = $("#j-scroll span.btn-right"),
            li_width = $("#j-scroll li").outerWidth(true);
        $ul.html($ul.html() + $ul.html());
        $ul.width(li_width * $("#j-scroll li").length);
        $(".button span").hover(function () {
            $(this).addClass("hover");
        }, function () {
            $(this).removeClass("hover");
        });
        $lf.click(function () {
            if ($ul.position().left >= 0) {
                $ul.css({
                    left: -$ul.width() / 2
                });
            }
            $ul.stop().animate({
                left: "+=" + li_width * 3
            }, "slow");

        });
        $rt.click(function () {
            if ($ul.position().left <= -$ul.width() / 2) {
                $ul.css({
                    left: 0
                });
            }
            $ul.stop().animate({
                left: "-=" + li_width * 3
            });
        });
        var timer = setInterval(function () {
            if ($ul.position().left <= -$ul.width() / 2) {
                $ul.css({
                    left: 0
                });
            } else {
                $ul.stop().animate({
                    left: "-=" + li_width
                }, "slow");
            }
        }, 3500);
        $("#j-scroll").hover(function () {
            clearInterval(timer);
        }, function () {
            timer = setInterval(function () {
                if ($ul.position().left <= -$ul.width() / 2) {
                    $ul.css({
                        left: 0
                    });
                } else {
                    $ul.stop().animate({
                        left: "-=" + li_width
                    }, "slow");
                }
            }, 3500);
        });
    });
    </script>

